
#include <stdio.h>
#include <stdlib.h>


int utn_validateArrayChar(char *array);

void main(void)
{
int v[60],i;
for (i=0;i<5;i++)
{
printf("Ingrese valor a cargar en el vector");
scanf("%d",&v[i]);
}
}




int utn_validateArrayChar(char *array){
    int i;
    int isArrayChar = 0;
  if (array != NULL){
      for (i = 0; i < strlen (array) - 1; i++){
	  if ((array[i] >= 'A' && array[i] <= 'Z')
	      || (array[i] >= 'a' && array[i] <= 'z')){
		  isArrayChar = 0;
	    }else{
	      isArrayChar = -1;
	      break;
	   }

	}
  }
    return isArrayChar;
}
