int cargar(char * array ){

	char buffer;
	int retorno ;
	retorno = -1;

	if ( array !=NULL){
	int nombre[60],i;
	for (i=0;i<60;i++){
		scanf("%c",&nombre[i]);
	}
	return retorno;
}

int utn_validateArrayChar(char *array){
    int i;
    int isArrayChar = 0;
  if (array != NULL){
      for (i = 0; i < strlen (array) - 1; i++){
	  if ((array[i] >= 'A' && array[i] <= 'Z')
	      || (array[i] >= 'a' && array[i] <= 'z')){
		  isArrayChar = 0;
	    }else{
	      isArrayChar = -1;
	      break;
	   }

	}
  }
    return isArrayChar;
}
